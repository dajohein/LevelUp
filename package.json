{
  "name": "levelup",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:storage": "node scripts/dev-storage-server.cjs",
    "dev:full": "concurrently \"npm run dev:storage\" \"npm run dev\" --names \"api,web\" --prefix-colors \"blue,green\"",
    "dev:start": "./scripts/dev-start.sh",
    "dev:stop": "./scripts/dev-stop.sh",
    "dev:restart": "./scripts/dev-restart.sh",
    "build": "tsc && vite build",
    "build:production": "./scripts/build-with-versioning.sh",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 50",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "health:storage": "curl -s http://localhost:5173/api/health || echo 'Development environment not running. Start with: npm run dev:start'",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "icons:generate": "bash scripts/pwa/generate-icons.sh",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/uuid": "^11.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.30.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vercel/node": "^5.5.15",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.1",
    "express": "^5.2.1",
    "prettier": "^3.7.4",
    "typescript": "^5.0.4",
    "vite": "^7.2.6",
    "vite-plugin-remove-console": "^2.2.0"
  }
}
